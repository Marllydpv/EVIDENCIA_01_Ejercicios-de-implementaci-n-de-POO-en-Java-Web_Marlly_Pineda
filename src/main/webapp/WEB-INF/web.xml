<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <welcom-file-list>
    <welcom-file>index.jsp </welcom-file>
  </welcom-file-list>
  <servlet>
    <servlet-name>Objeto</servlet-name>
    <servlet-class>Objeto</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Objeto</servlet-name>
    <url-pattern>/objeto</url-pattern>
  </servlet-mapping>  
  <servlet>
    <servlet-name>AgregarHistoriaServlet</servlet-name>
    <servlet-class>com.example.AgregarHistoriaServlet</servlet-class>
</servlet>
<servlet>
    <servlet-name>EliminarHistoriaServlet</servlet-name>
    <servlet-class>com.example.EliminarHistoriaServlet</servlet-class>
</servlet>

<servlet-mapping>
    <servlet-name>AgregarHistoriaServlet</servlet-name>
    <url-pattern>/agregarHistoria</url-pattern>
</servlet-mapping>
<servlet-mapping>
    <servlet-name>EliminarHistoriaServlet</servlet-name>
    <url-pattern>/eliminarHistoria</url-pattern>
</servlet-mapping>


</web-app>

